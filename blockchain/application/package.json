{
    "name": "asset-transfer-basic",
    "version": "1.0.0",
    "description": "Asset Transfer Basic Application implemented in typeScript using fabric-gateway",
    "main": "dist/server.js",
    "typings": "dist/server.d.ts",
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc -w",
        "lint": "eslint . --ext .ts",
        "prepare": "npm run build",
        "pretest": "npm run lint",
        "start": "node --require dotenv/config dist/server.js"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "@grpc/grpc-js": "^1.5.0",
        "@hyperledger/fabric-gateway": "^1.0.0",
        "dotenv": "^16.0.0",
        "env-var": "^7.1.1",
        "express": "^4.17.3",
        "helmet": "^5.0.2"
    },
    "devDependencies": {
        "@tsconfig/node14": "^1.0.1",
        "@types/express": "^4.17.13",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "eslint": "^8.4.1",
        "typescript": "~4.5.2"
    }
}
